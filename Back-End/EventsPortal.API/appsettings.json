{
  "Settings": {
    "ConnectionSettings": {
      "DBConnectionType": "SqlLite",      
      "DBConnectionString": "Filename=EventsDatabase.db",
      "MigrationAssemblyName": "EventsPortal.DAL"
    },
    "Cors": {
      "Hubs": [
        "http://localhost:4200"        
      ],
      "Headers": [
        "authorization",
        "content-type"
      ]
    },
    "Authentication": {
      "Audience": "eventsportal.api",
      "RequireHttpsMetadata": false,
      "Authority": "http://auth.example.org"
    },
    "BLLAssembly":  "EventsPortal.BLL"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}
